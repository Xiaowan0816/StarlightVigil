# Auto detect text files and perform LF normalization
* text=auto
*.psd filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.fbx filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
/Library/ArtifactDB filter=lfs diff=lfs merge=lfs -text
/Library/PackageCache/com.unity.burst@1.8.21/.Runtime/libburst-llvm-18.dylib filter=lfs diff=lfs merge=lfs -text
/Library/PackageCache/com.unity.burst@1.8.21/.Runtime/libburst-llvm-19.dylib filter=lfs diff=lfs merge=lfs -text
